{
	"env": {
		"browser": true,
		"commonjs": true,
    "es6": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"overrides": [
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"error",
			2
		],
		"linebreak-style": [
			"error",
			"windows"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"never"
		],
    "react/prop-types": 0,
    "no-var": 2,
    "no-trailing-spaces": 2,
    "eqeqeq": 2,
    "react/react-in-jsx-scope": 0
	},
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "18.2.0"
    }
  }
}
